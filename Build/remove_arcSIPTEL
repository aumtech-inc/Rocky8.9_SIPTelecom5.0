#-------------------------------------------------------------------------
# This function requests relevant information from the user before allowing
# the removal to proceed. In particular, it gets the user id under which
# Telecom Services was installed.
#-------------------------------------------------------------------------
request()
{

	if ! echo $HOME | grep "root" >/dev/null
	then
		echo "You must be root to remove SIP Telecom Services. "
		echo "Removal aborted."
		exit 3
	fi

	if      ps -ef | grep "ArcIPDynMgr" | grep -v grep >/dev/null
	then
		echo "ERROR: SIP Telecom Services appears to be running."
		echo "Please stop it before proceeding."
                echo "Removal aborted."
                exit 3
        fi

        if  !   tail -1 $PKGINFO | grep "INSTALL" | grep -v grep >/dev/null
        then
              echo "ERROR: SIP Telecom Services does not appear to be installed."
                show_pkginfo
                echo "Removal aborted."
                exit 3
        fi

while :
do
	echo
	echo -n "User id under which you installed SIP Telecom Services $VERSION [default: arc] : "
	read USER_ID
	if [ "$USER_ID" = "" ]
	then
		USER_ID=arc
	fi

	if [ `grep -c "^$USER_ID:" /etc/passwd` -ne 1 ]
	then
		echo "Error: invalid username ($USER_ID) entered.  Please try again..."
		echo
		continue
	fi
	break
done
}

update_pkginfo()
{
	STATUS=$1
	REMOVEDATE=`date +"%Y %B %d %X"`

	echo "--------------------------" >> $PKGINFO
	echo 				  >> $PKGINFO
	echo "Caution: Do not remove or edit this file." >> $PKGINFO
	echo "PKGINST   : $PKGINST"       >> $PKGINFO
	echo "VERSION   : $VERSION"       >> $PKGINFO
	echo "USER      : $USER_ID"       >> $PKGINFO
	echo "REMOVEDATE: $REMOVEDATE"    >> $PKGINFO
	echo "STATUS    : $STATUS"        >> $PKGINFO
}

show_pkginfo()
{
	echo Showing $PKGINFO...
        tail -11 $PKGINFO
}

#-----------------------------------------------------------------------------
# main routine
#-----------------------------------------------------------------------------

# Packing information
PKGINST="arcSIPTEL"
VERSION="5.0"
BUILD="2"
PKGINFO="/home/.$PKGINST.pkginfo" 

request

IU=/home/$USER_ID/.ISP/Global/Exec 	# Location of install utilities.
echo -n "About to remove SIP Telecom Services $VERSION. Are you sure? (y/[n]) "
read answer
if [ "$answer" = "y" ]
then 
	:
else
	echo "Removal aborted."
	exit 3
fi
                             
Echo "`date` Removing SIP Telecom Services ..."

ISPBASE=/home/$USER_ID/.ISP

#
# Stop the licensing
#
echo "y\n" | $ISPBASE/Global/Exec/lmutil lmdown -c $ISPBASE/Global/Tables/license.dat >/dev/null 2>&1

Remove_rc_scripts

Remove_global_files
Remove_crontab_entries

Remove_files

initctl stop Aumtech-Redirector 2>/dev/null
systemctl stop Aumtech-Redirector.service 2>/dev/null

rm -f /etc/systemd/system/Aumtech-Redirector.service 2>/dev/null
rm -f /etc/systemd/system/Aumtech-Redirector.service.noRun 2>/dev/null

update_pkginfo REMOVED
echo 
echo "SIP Telecom Services has been removed."
echo
