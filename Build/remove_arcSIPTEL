#---------------------------------------------------------------------------
# Program:	remove_arcSIPTEL
# Purpose:	Script to remove a SIP Telecom Services package. 
# Author:	Aumtech, Inc.
#---------------------------------------------------------------------------

Echo()
{
	echo "$1" 
}

replace_token()
{
    FILE=$1
    TOKEN1=$2
    TOKEN2=$3
    if `grep "$TOKEN1" $FILE > /dev/null 2>&1 `
    then
#       echo DEBUG REPLACING $TOKEN1 with $TOKEN2
        echo "/$TOKEN1"         >  e$$
        echo "d"            >> e$$
        echo "."            >> e$$
        echo "i"            >> e$$
        echo "$TOKEN2"          >> e$$
        echo "."            >> e$$
        echo "w\nq"             >> e$$
        ed $FILE < e$$ > /dev/null 2>/dev/null
        rm -f e$$
    else
#       echo DEBUG NOT FOUND $TOKEN1
        if [ "$4" =  "INSERT" ]
        then
#           echo DEBUG INSERTING $TOKEN2
            echo "."            >> e$$
            echo "i"            >> e$$
            echo "$TOKEN2"          >> e$$
            echo "."            >> e$$
            echo "w\nq"             >> e$$
            ed $FILE < e$$ > /dev/null 2>/dev/null
            rm -f e$$
        fi
    fi
}

#----------------------------------------------------------------------------
# Determine what other ARC packages are installed. 
#----------------------------------------------------------------------------
Determine_whats_installed()
{
	for pkg in $ARC_PKGS
	do
		if [ "$pkg" = "arcSNA" ]
		then
			if [ -f /home/$USER_ID/.ISP/SNA/Exec/isp_snaresp ]
			then
				Echo "Found $pkg installed."
				Other_ARC_pkgs=1
			fi
		fi
		if [ "$pkg" = "arcWSS" ]
		then
			if [ -f /home/$USER_ID/.ISP/WSS/Exec/wss_listen ]
			then
				Echo "Found $pkg installed."
				Other_ARC_pkgs=1
			fi
		fi
		if [ "$pkg" = "arcTCP" ]
		then
			if [ -f /home/$USER_ID/.ISP/TCP/Exec/isp_tcpresp ]
			then
				Echo "Found $pkg installed."
				Other_ARC_pkgs=1
			fi
		fi
	done
	
	if [ -f /home/$USER_ID/.ISP/Network/Exec/isp_netserv ]
	then
		Other_ARC_pkgs=1
		Network_services_installed=1 
		Echo "Found ARC Network Services installed."
	fi
}

Remove_global_files()
{
	Echo "Removing global files..."
	GLOBAL=/home/$USER_ID/ISP
	CMD="rm"
	
	$CMD /home/$USER_ID/.isprc
	
	
#	Flexlm license files	
	$CMD $GLOBAL/Exec/arcLicense
	$CMD $GLOBAL/Exec/arclicd
	$CMD $GLOBAL/Exec/lmgrd
	$CMD $GLOBAL/Exec/lmutil
	$CMD $GLOBAL/Exec/chkSRLicense

	$CMD $GLOBAL/Exec/isplogrc 				
	$CMD $GLOBAL/Exec/arczip.sh

	$CMD $GLOBAL/Exec/log_cli
	$CMD $GLOBAL/Exec/log_forward
	$CMD $GLOBAL/Exec/Start_LogForward
	$CMD $GLOBAL/Exec/arcLogCleanup
	$CMD $GLOBAL/Exec/arcRTRClient

	$CMD $GLOBAL/include/ISPCommon.h 		
	$CMD $GLOBAL/include/UTL_accessories.h 		
	$CMD $GLOBAL/include/gaUtils.h
	
	$CMD $GLOBAL/lib/libISPLog.a 			
	$CMD $GLOBAL/lib/libUTILS.a 			
	$CMD $GLOBAL/lib/libISPCDR.a 			
	$CMD $GLOBAL/lib/libISPUtil.a 			
	$CMD $GLOBAL/lib/libgaUtils.a
	
#	$CMD $GLOBAL/.Utilities/Current_LogParams 

}

Remove_rc_scripts()
{
	Echo "Removing Telecom Startup Mechanism."
    if [ `grep -c S99arciptel /etc/rc.d/rc.local` -eq 1 ]
    then 
		grep -v S99arciptel /etc/rc.d/rc.local >/tmp/tc99
		mv /tmp/tc99  /etc/rc.d/rc.local 2>/dev/null
		chmod 755 /etc/rc.d/rc.local
		rm /tmp/tc99 2>/dev/null
    fi
}
Remove_crontab_entries()
{
        Echo "Removing crontab entries..."

        CRON_DIR="/var/spool/cron"

	FILE="$CRON_DIR/$USER_ID"
	if [ -f $FILE ]
	then
		EXECUTABLE="/home/$USER_ID/.ISP/Global/Exec/Start_LogForward"
		EXECUTABLE2="/home/$USER_ID/.ISP/Global/Exec/arcLogCleanup"
		EXECUTABLE3="/home/$USER_ID/.ISP/Global/Exec/arczip.sh"
		cat $FILE | egrep -v "$EXECUTABLE|$EXECUTABLE2|$EXECUTABLE3" > $FILE.tmp
		if [ -s $FILE.tmp ]
		then
			# There was "keepable" stuff in original cron file
			# so move the good stuff back.
			mv $FILE.tmp $FILE
		else
			# There was NO "keepable" stuff in original cron file
			# so just delete the original file & the tmp file.
			rm $FILE $FILE.tmp
		fi
	fi
}

Remove_environment_setup_from_profile()
{
	# Remove the .isprc line from the profile
	ENV_SETUP_FILE=".isprc"
	PROFILE="/home/$USER_ID/.bash_profile"

	if grep "$ENV_SETUP_FILE" $PROFILE  1>/dev/null 2>&1
	then
		echo "Removing call to $ENV_SETUP_FILE from $PROFILE."
		echo "/$ENV_SETUP_FILE\nd\n.\nw\nq" > e.$$
        	ed $PROFILE < e.$$ >/dev/null 2>/dev/null
		rm e.$$ >/dev/null 2>/dev/null 
	fi
}

Remove_arciptelrc_from_profile()
{
Echo "Removing call to arciptelrc from /home/$USER_ID/.profile"
$IU/replace_token /home/$USER_ID/.profile ". \/home\/$USER_ID\/.arciptelrc" \# 
}


RemoveDir()
{
	rmdir $1 1>/dev/null 2>/dev/null
}

Remove()
{
	rm -f $* 1>/dev/null 2>/dev/null
}


Remove_directories()
{
	echo "Removing Telecom Services directories..."
	RemoveDir /home/$USER_ID/.ISP/Telecom/Exec
	RemoveDir /home/$USER_ID/.ISP/Telecom/Operations
	RemoveDir /home/$USER_ID/.ISP/Telecom/Tables
	RemoveDir /home/$USER_ID/.ISP/Telecom/phrases
	RemoveDir /home/$USER_ID/.ISP/Telecom/locks
	RemoveDir /home/$USER_ID/.ISP/Telecom
}

